{
	"schemaVersion": 1,
	"id": "context",
	"version": "${version}",
	"name": "Context",
	"description": "A Fabric library mod for serializing/deserializing custom text contents.",
	"authors": [
		"eggohito"
	],
	"contact": {
		"sources": "https://github.com/eggohito/context"
	},
	"license": "CC0-1.0",
	"icon": "assets/context/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.eggohito.context.Context"
		],
		"preLaunch": [
			"io.github.eggohito.context.integration.ContextIntegration::registerVanilla"
		]
	},
	"mixins": [
		"context.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	}
}